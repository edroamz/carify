{
  "homepage": "https://edroamz.github.io/carlify",
  "name": "carlify",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "framer-motion": "^1.11.1",
    "react": "^16.13.1",
    "react-cool-img": "^1.2.12",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "npm run build:css && react-scripts start",
    "build": "npm-run-all build:css build:js && cp build/index.html build/404.html",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:js": "react-scripts build",
    "build:css": "postcss src/styles/tailwind.css -o src/styles/main.css --env production"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^9.8.5",
    "gh-pages": "^3.2.3",
    "postcss-cli": "^7.1.1",
    "tailwindcss": "^1.4.6"
  }
}
